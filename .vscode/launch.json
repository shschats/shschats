{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Next: Dev",
        "program": "${workspaceFolder}/node_modules/.bin/next",
        "args": ["dev"],
        "autoAttachChildProcesses": true,
        "console": "integratedTerminal"
